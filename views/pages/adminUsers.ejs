<html lang="en" dir="ltr">
  <head>
    <!-- Power by Nikolay Vasilev! -->
    <meta charset="utf-8">
    <meta name="description" content="This is a web site where I create web site designs">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="keywords" content="design, Art, web, site">
    <meta name="author" content="Nikolay Vasilev">
    <link rel="icon" href="<%= headIcon %>" type="image/x-icon">
    <title><%= headTitle %></title>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css">
    <!-- Master Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Archivo+Narrow" rel="stylesheet">
    <!-- My CSS -->
    <link rel="stylesheet" href="<%= myCSS %>">
    <!-- Bootstrap core CSS -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.min.css" rel="stylesheet">
    <!-- Material Design Bootstrap -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.6.1/css/mdb.min.css" rel="stylesheet">
  </head>
  <body>

    <!--Navbar-->
    <nav class="navbar navbar-expand-lg navbar-dark blue-grey darken-4">
      <div class="container">
        <!-- Navbar brand -->
        <img src="<%= brand %>" alt="brand" width="40px" height="40px">
        <a class="navbar-brand text-default text-uppercase pl-2" href="/admin">Admin panel</a>

        <!-- Collapse button -->
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#basicExampleNav"
          aria-controls="basicExampleNav" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>

        <!-- Collapsible content -->
        <div class="collapse navbar-collapse" id="basicExampleNav">

          <!-- Links -->
          <ul class="navbar-nav ml-auto">
            <li class="nav-item <%= m1 %>">
              <a class="nav-link text-default text-uppercase" href="/admin">Home
                <span class="sr-only">(current)</span>
              </a>
            </li>
            <li class="nav-item <%= m2 %>">
              <a class="nav-link text-default text-uppercase" href="/adminUsers">Users</a>
            </li>
          </ul>
          <!-- Links -->

          <ul class="navbar-nav nav-flex-icons">
            <li class="nav-item dropdown">
              <a class="nav-link dropdown-toggle text-default" id="navbarDropdownMenuLink-333" data-toggle="dropdown" aria-haspopup="true"
                aria-expanded="false">
                <i class="fas fa-user"></i>
              </a>
              <div class="dropdown-menu dropdown-menu-center dropdown-default blue-grey darken-4" aria-labelledby="navbarDropdownMenuLink-333">
                <a class="dropdown-item text-default text-uppercase" href="/logout">Logout</a>
              </div>
            </li>
          </ul>

        </div>
        <!-- Collapsible content -->
      </div>
    </nav>
    <!--/.Navbar-->

    <main class="container-fluid mt-3">
      <div class="row">
        <div class="col-md-12 text-center">
          <h2>All Users</h2>
        </div>
      </div>
      <div class="row">
        <table class="table table-bordered mx-5">
            <thead class="thead-light">
              <tr>
                <th scope="col">ID</th>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                <th scope="col">Password</th>
                <th scope="col">Created_at</th>
                <th scope="col">Updated_at</th>
                <th scope="col">Level</th>
              </tr>
            </thead>
            <tbody class="text-black">
              <% for(var i=0; i<= UsersTable.length-1; i++) { %>
                <tr>
                  <td><%= UsersTable[i].id %></td>
                  <td><%= UsersTable[i].name %></td>
                  <td><%= UsersTable[i].email %></td>
                  <td><%= UsersTable[i].password %></td>
                  <td><%= UsersTable[i].created_at %></td>
                  <td><%= UsersTable[i].updated_at %></td>
                  <td><%= UsersTable[i].level %></td>
                </tr>
              <% } %>
            </tbody>
          </table>
      </div>
    </main>

    <!-- Footer -->
    <footer class="page-footer font-small blue-grey darken-4 mt-3">

      <!-- Copyright -->
      <div class="footer-copyright text-center text-default py-3">Â© 2019 Copyright:
        <a href="/admin">Mobile and web technologies, Group: 37</a>
      </div>
      <!-- Copyright -->

    </footer>
    <!-- Footer -->

    <!-- SCRIPTS -->
    <!-- JQuery -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <!-- Bootstrap tooltips -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.4/umd/popper.min.js"></script>
    <!-- Bootstrap core JavaScript -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/js/bootstrap.min.js"></script>
    <!-- MDB core JavaScript -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.6.1/js/mdb.min.js"></script>
    <!-- My SCRIPTS -->

  </body>
</html>
